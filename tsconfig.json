{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "*": [
        "./*"
      ],
      "root/*": [
        "./*"
      ],
      "src/*": [
        "src/*"
      ],
      "assets/*": [
        "./src/assets/*"
      ],
      "navigation": [
        "./src/navigation"
      ],
      "screens": [
        "./src/screens"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
